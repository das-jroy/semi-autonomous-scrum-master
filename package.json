{
  "name": "@your-org/semi-autonomous-scrum-master",
  "version": "1.0.0",
  "description": "AI-powered semi-autonomous scrum master with enterprise orchestration",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:clean": "rm -rf dist && npm run build",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "test": "jest --config jest.config.js",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:verbose": "jest --verbose",
    "test:basic": "jest tests/basic.test.ts",
    "lint": "eslint src/**/*.ts tests/**/*.ts",
    "lint:fix": "eslint src/**/*.ts tests/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\" \"*.{js,json,md}\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\" \"*.{js,json,md}\"",
    "type-check": "tsc --noEmit",
    "type-check:test": "tsc --project tsconfig.test.json --noEmit",
    "validate": "npm run type-check && npm run lint && npm run format:check && npm run test",
    "pre-commit": "npm run lint:fix && npm run format && npm run type-check && npm run test",
    "prepare": "npm run build"
  },
  "keywords": [
    "scrum",
    "agile",
    "github",
    "automation",
    "ai",
    "copilot",
    "project-management"
  ],
  "author": "Your Organization",
  "license": "MIT",
  "dependencies": {
    "@octokit/auth-app": "^6.0.0",
    "@octokit/rest": "^20.0.0",
    "chalk": "^5.3.0",
    "class-transformer": "^0.5.0",
    "class-validator": "^0.14.0",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "fs-extra": "^11.2.0",
    "inquirer": "^9.2.12",
    "node-fetch": "^3.3.2",
    "openai": "^4.0.0",
    "ora": "^7.0.1",
    "reflect-metadata": "^0.1.13",
    "winston": "^3.10.0",
    "yaml": "^2.3.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.0.0",
    "@types/nodemailer": "^6.4.17",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "axios": "^1.10.0",
    "eslint": "^8.0.0",
    "husky": "^9.1.7",
    "jest": "^29.0.0",
    "jest-junit": "^16.0.0",
    "lint-staged": "^16.1.2",
    "nodemailer": "^7.0.5",
    "prettier": "^3.0.0",
    "ts-jest": "^29.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0",
    "whatwg-fetch": "^3.6.20"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
