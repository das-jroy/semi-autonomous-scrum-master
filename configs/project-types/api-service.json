{
  "name": "API Service",
  "description": "REST API or microservice development project",
  "categories": ["api", "database", "security", "testing", "documentation"],
  "default_sprints": 4,
  "complexity_factors": {
    "simple": 1,
    "medium": 2,
    "complex": 5,
    "epic": 8
  },
  "issue_templates": {
    "endpoint": {
      "title_prefix": "API:",
      "labels": ["api", "enhancement"],
      "body_template": "## Endpoint\n\n## Request/Response\n\n## Business Logic\n\n## Testing\n"
    },
    "integration": {
      "title_prefix": "Integration:",
      "labels": ["integration", "task"],
      "body_template": "## Service Description\n\n## Integration Points\n\n## Configuration\n"
    }
  }
}
