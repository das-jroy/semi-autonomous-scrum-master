{
  "name": "Infrastructure Project",
  "description": "Cloud infrastructure and DevOps automation project",
  "categories": ["infrastructure", "automation", "monitoring", "security", "compliance"],
  "default_sprints": 8,
  "complexity_factors": {
    "simple": 2,
    "medium": 5,
    "complex": 8,
    "epic": 13
  },
  "issue_templates": {
    "module": {
      "title_prefix": "Module:",
